<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/header.ejs") %>
</head>
<body>
    <%- include('../partials/nav.ejs') %>

    <h1>Houses for Sale</h1>
    <% listings.forEach(listing => { %>
        <img class="house-listing" src="/uploads/<%= listing.img %>" alt="house-id#-<%=listing._id%>">
        <ul class="house-listing">
            <li><a href="/listings/<%=listing._id%>"><%= listing.address %></a></li>
            <li><%= listing.city %></li>
            <li><%= listing.price %> </li>
        </ul>
    <% }) %>
</body>
</html>